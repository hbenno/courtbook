# CourtBook environment variables
# Copy to .env and adjust as needed. Docker Compose sets these automatically
# for containers, but you may want a .env for running scripts outside Docker.

CB_DATABASE_URL=postgresql+asyncpg://courtbook:courtbook@localhost:5432/courtbook
CB_REDIS_URL=redis://localhost:6379/0
CB_DEBUG=true
CB_SECRET_KEY=dev-secret-change-in-production

# Stripe (test mode - get keys from https://dashboard.stripe.com/test/apikeys)
CB_STRIPE_SECRET_KEY=
CB_STRIPE_WEBHOOK_SECRET=
